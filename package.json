{
  "name": "aurora-e2e-tests",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": ".eslintrc.js",
  "scripts": {
    "lint": "eslint . --ext .js,.cjs,.ts",
    "typecheck": "tsc --noEmit",
    "cache": "synpress",
    "test": "playwright test --workers=1",
    "test-aurora": "playwright test --workers=1 --grep @aurora-plus",
    "test-web-wallet": "playwright test --workers=1 --grep @web3-wallet",
    "test-cloud-console": "playwright test --workers=1 --grep @aurora-cloud-console",
    "ui-mode": "playwright test --ui --workers=1",
    "test-tag": "playwright test --workers=1 --grep",
    "debug": "yarn playwright test --debug",
    "console": "PWDEBUG=console yarn playwright test",
    "report": "yarn playwright show-report"
  },
  "author": "Alex Mendes",
  "license": "UNLICENSED",
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "@estruyf/github-actions-reporter": "^1.10.0",
    "@slack/types": "^2.14.0",
    "@supabase/supabase-js": "^2.49.4",
    "@synthetixio/synpress": "4.0.3",
    "dotenv": "16.4.5",
    "playwright": "1.44.0",
    "playwright-slack-report": "^1.1.89"
  },
  "devDependencies": {
    "@playwright/test": "1.44.0",
    "@types/node": "22.3.0",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "3.3.3",
    "typescript": "5.5.4"
  },
  "resolutions": {
    "strip-ansi": "^6"
  }
}
